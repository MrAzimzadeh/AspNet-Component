@model List<Messagess>
<table class="table">
    <thead>
        <tr>
            <th class="col">Id</th>
            <th class="col">Ad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id </td>
                <td>@item.messages</td>
            </tr>
        }
    </tbody>
</table>